  	<select id="getSShippingPlanByID" resultMap="getSShippingPlan" parameterType="java.lang.Integer">
  		SELECT
			sp.*,
			spd.id sid,
			spd.ch_item_name,
			spd.domestic_source_of_goods,
			spd.currency,
			spd.hscode,
			spd.item_number,
			spd.s_id,
			spd.model,
			spd.net_weight,
			spd.note,
			spd.number,
			spd.price,
			spd.serial_number,
			spd.total_price,
			spd.unit
		FROM
			s_shipping_plan sp
		LEFT JOIN s_shipping_plan_details spd ON sp.id = spd.s_id
		WHERE
			sp.id = #{id}
  	</select>
  	
